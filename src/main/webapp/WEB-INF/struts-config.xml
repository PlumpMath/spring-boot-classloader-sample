<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">


<struts-config>

  <global-forwards>
    <forward
            name="welcome"
            path="/welcome.do"/>

  </global-forwards>


  <action-mappings>
    <action
            path="/welcome"
            forward="/pages/Welcome.jsp"/>

  </action-mappings>



</struts-config>

